        {
        	$this->checkLoaded<{$property->getPhpName()}>();
        	$propertyBag['<{$property->getName()}>'] = $this->m_<{$property->getName()}>;
        }
        elseif ($this->m_<{$property->getName()}> !== null && !is_numeric($this->m_<{$property->getName()}>))
        {
        	$propertyBag['<{$property->getName()}>'] = $this->m_<{$property->getName()}>;
